{
  "name": "jedlik-vite-ts-tamplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --port 8080 --open",
    "dev-clear": "vite --force --port 8080 --open",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest run",
    "test-coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "axios": "^0.26.0",
    "bootstrap": "^5.1.3",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.9",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^12.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vite-plugin-eslint": "^1.3.0",
    "vitest": "^0.5.5",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.32.0"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/nitslaszlo/jedlik-vite-ts-template"
  },
  "license": "MIT"
}
