{
  "name": "jedlik-vite-ts-tamplate",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite  --port 8080 --open",
    "dev-clear": "vite --force --port 8080 --open",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --ignore-path .gitignore --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "test": "vitest run"
  },
  "dependencies": {
    "@mdi/font": "^6.5.95",
    "axios": "^0.24.0",
    "roboto-fontface": "^0.10.0",
    "vue": "^3.2.29",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12",
    "vue3-table-lite": "^1.1.5",
    "vuetify": "^3.0.0-alpha.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/test-utils": "^2.0.0-rc.18",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4",
    "vite": "^2.7.13",
    "vite-plugin-eslint": "^1.3.0",
    "vitest": "^0.1.24",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.30.1"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/nitslaszlo/jedlik-vite-ts-template"
  },
  "license": "MIT"
}
